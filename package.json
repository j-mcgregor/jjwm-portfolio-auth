{
  "name": "jjwm-portfolio-auth",
  "version": "1.0.0",
  "description": "The API for this portfolio",
  "main": "src/index.js",
  "scripts": {
    "build": "babel src -d dist --source-maps",
    "eslint": "eslint src/**/*.js --ignore-pattern \"node_modules/\"",
    "lint": "esw . --color",
    "lint:fix": "esw . --color --fix",
    "lint:watch": "npm run lint -- --watch",
    "seed": "nodemon ./seed/index.js --exec babel-node --presets @babel/preset-env",
    "serve": "node dist/index.js",
    "start": "nodemon src/index.js --exec babel-node --presets @babel/preset-env",
    "test": "cross-env NODE_ENV=test TZ=utc jest",
    "test:cover": "npm run test -- --coverage ",
    "test:report": "npm run test --reporters=\"jest-mocha-reporter\" ",
    "test:watch": "jest --watchAll"
  },
  "author": "Jack McGregor",
  "license": "ISC",
  "dependencies": {
    "@hapi/joi": "^16.1.8",
    "@shelf/jest-mongodb": "^1.1.3",
    "babel-eslint": "^10.0.3",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.1",
    "body-parser": "^1.19.0",
    "chalk": "^3.0.0",
    "cookie-parser": "^1.4.4",
    "cookie-session": "^1.3.3",
    "cors": "^2.8.5",
    "cross-env": "^6.0.3",
    "dotenv": "^8.2.0",
    "eslint": "^6.7.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.0.4",
    "eslint-plugin-node": "^10.0.0",
    "eslint-watch": "^6.0.1",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "graphql": "^14.5.8",
    "helmet": "^3.21.2",
    "jest-cli": "^24.9.0",
    "jest-mocha-reporter": "^1.0.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.12",
    "morgan": "^1.9.1",
    "node-fetch": "^2.6.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "prettier": "^1.19.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/node": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-register": "^6.26.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "faker": "^4.1.0",
    "jest": "^24.9.0",
    "mock-req-res": "^1.1.3",
    "nodemon": "^2.0.1",
    "sinon": "^7.5.0",
    "sinon-express-mock": "^2.2.1",
    "supertest": "^4.0.2"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": true
          }
        }
      ]
    ]
  },
  "engines": {
    "node": ">=8"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "eslint:recommended",
      "plugin:import/errors",
      "plugin:import/warnings",
      "plugin:react/recommended"
    ],
    "plugins": [
      "react"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true
      }
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true,
      "jquery": true,
      "jest": true
    },
    "globals": {}
  }
}
